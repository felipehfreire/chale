<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    
    <navigation-rule>
     	<from-view-id>consultarVenda.xhtml</from-view-id>
          <navigation-case>
               <from-outcome>finalizar</from-outcome>
                    <to-view-id>finalizarVenda.xhtml</to-view-id>
                    <redirect></redirect>
          </navigation-case>
	</navigation-rule>
	
	<navigation-rule>
     	<from-view-id>consultarVendaAPrazo.xhtml</from-view-id>
			<navigation-case>
               <from-outcome>detalhar</from-outcome>
                    <to-view-id>detalharVendaPrazo.xhtml</to-view-id>
                    <redirect></redirect>
          </navigation-case>
    </navigation-rule>
          

</faces-config>
